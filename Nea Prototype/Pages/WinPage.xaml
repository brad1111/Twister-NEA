<Page x:Class="Nea_Prototype.Pages.WinPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Nea_Prototype.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="WinPage">
    <Grid Background="Black">
        <Grid Background="#99ffff00">
            <StackPanel HorizontalAlignment="Center"  VerticalAlignment="Top" Margin="0,40,0,0">
                <Label Foreground="White" FontSize="40"  HorizontalAlignment="Center">
                    You have won!
                </Label>
                <Label HorizontalContentAlignment="Center" Foreground="White">In Time: {}</Label>
            </StackPanel>
            <Button x:Name="btnContinue" HorizontalAlignment="Right" VerticalAlignment="Bottom" Click="BtnContinue_OnClick">
                Continue
            </Button>
        </Grid>    
    </Grid>
</Page>
